{
  "name": "php-mohamed-nabil/marrow-framework",
  "description": "marrow is a php MVC Framwork for building php web application with a good mvc pattern structure makes development more easier",
  "keywords": ["mvc","php","php8","php-framwork","oop","mysql","php-mvc","api"],
  "type": "project",
  "require": {
    "respect/validation": "^2.2",
    "intervention/image": "^2.7",
    "nesbot/carbon": "^2.67",
    "rakit/validation": "dev-master",
    "league/pipeline": "dev-master",
    "optimus/onion": "~1.0",
    "vlucas/phpdotenv": "dev-master",
    "verot/class.upload.php": "dev-master",
    "doctrine/inflector": "2.0.x-dev",
    "symfony/password-hasher": "5.4.x-dev",
    "php-mohamed-nabil/style": "dev-master",
    "league/booboo": "dev-main",
    "spatie/ignition": "^1.9",
    "httpsoft/http-server-request": "dev-master",
    "league/oauth2-facebook": "dev-main"
    },
    "autoload":
  {
     "files":
     [
         "src/coremvc/functions.php",
         "src/coremvc/process.php"
     ],
